/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
    background: #f9f9f9;
}




/* 模块楼层样式 */

.module_head {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 140rpx;
  background: #ffffff;
  margin-top: 15px;
  border-bottom: 1px solid #f7f7f7;
}
.module_head_content {
  width: 100%;
}
.module_head_title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 36rpx;
  color: #333;
}
.module_head_title::before {
  content: '';
  display: inline-block;
  width: 56rpx;
  height: 34rpx;
  margin-right: 6px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.module_head_title::after {
  content: '';
  display: inline-block;
  width: 64rpx;
  height: 34rpx;
  margin-left: 6px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.module_head_desc {
  text-align: center;
  font-size: 20rpx;
  color: #999;
}
.module_head_more {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  right: 30rpx;
  font-size: 12px;
  color: #999 !important;
}
.module_head_more_icon {
  display: inline-block;
  width: 14rpx;
  height: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}